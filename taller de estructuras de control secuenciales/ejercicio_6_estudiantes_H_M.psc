Algoritmo ejercicio_6_estudiantes_H_M
	//entradas
	Escribir "numero de mujeres"
	Leer mujeres
	Escribir "numero de hombres"
	Leer hombres
	//caja negra
	total_estudiante<-mujeres+hombres
	porMujeresz<-(mujeres/total_estudiante*100)
	porHombres<-(hombres/total_estudiante*100)
	//salidas
	Escribir "porcentaje de hombres", porHombres "%"
	Escribir "porcentaje de mujeres", porMujeresz "%"
	
FinAlgoritmo
